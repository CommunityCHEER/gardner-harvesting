{
  "name": "cheer-app",
  "main": "expo-router/entry",
  "version": "1.2.3",
  "scripts": {
    "devbuild": "npx eas build -e development -p android",
    "start": "npx tsc && expo start --tunnel",
    "start:web": "npx expo export --platform web && npx serve dist",
    "deploy:web": "npx expo export --platform web && npx firebase deploy --only hosting",
    "buildAndroid": "npx eas build --platform android --profile production",
    "buildIos": "npx eas build --platform ios --profile production",
    "submitAndroid": "npx eas submit -p android --latest --profile production",
    "submitIos": "npx eas submit --platform ios --latest --profile production",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^14.0.3",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-navigation/native": "^7.0.14",
    "expo": "~52.0.0",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.20",
    "expo-font": "~13.0.4",
    "expo-image-picker": "~16.0.6",
    "expo-linking": "~7.0.5",
    "expo-localization": "~16.0.1",
    "expo-router": "~4.0.21",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.9",
    "expo-updates": "~0.27.4",
    "expo-web-browser": "~14.0.2",
    "firebase": "^11.2.0",
    "i18n-js": "^4.4.3",
    "js-sha256": "^0.11.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-firebase-hooks": "^5.1.1",
    "react-native": "0.76.9",
    "react-native-calendars": "^1.1306.0",
    "react-native-dropdown-picker": "^5.4.6",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/firebase": "^2.4.32",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.6",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "private": true
}
