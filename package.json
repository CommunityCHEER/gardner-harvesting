{
  "name": "cheer-app",
  "main": "expo-router/entry",
  "version": "1.2.4",
  "scripts": {
    "expo:doctor": "npx expo-doctor",
    "start": "npx tsc && expo start --tunnel",
    "start:web": "EXPO_PUBLIC_GIT_HASH=$(git rev-parse --short HEAD) npx expo export --platform web && npx serve dist",
    "deploy:web": "EXPO_PUBLIC_GIT_HASH=$(git rev-parse --short HEAD) npx expo export --platform web && npx firebase deploy --only hosting",
    "build:android": "npx eas build --platform android --profile production",
    "build:ios": "npx eas build --platform ios --profile production",
    "submit:android": "npx eas submit -p android --latest --profile production",
    "submit:ios": "npx eas submit --platform ios --latest --profile production",
    "reset-project": "node ./scripts/reset-project.js",
    "devbuild": "npx eas build -e development -p android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-navigation/native": "^7.0.14",
    "expo": "~53.0.0",
    "expo-build-properties": "~0.14.8",
    "expo-constants": "~17.1.8",
    "expo-dev-client": "~5.2.4",
    "expo-font": "~13.3.2",
    "expo-image-picker": "~16.1.4",
    "expo-linking": "~7.1.7",
    "expo-localization": "~16.1.6",
    "expo-router": "~5.1.11",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.11",
    "expo-updates": "~0.28.18",
    "expo-web-browser": "~14.2.0",
    "firebase": "^11.2.0",
    "i18n-js": "^4.4.3",
    "js-sha256": "^0.11.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.6",
    "react-native-calendars": "^1.1306.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-toast-message": "^2.2.0",
    "react-native-web": "^0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/firebase": "^2.4.32",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.0.10",
    "@types/react-test-renderer": "^18.3.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.14",
    "react-test-renderer": "^19.0.0",
    "typescript": "~5.8.3",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "firebase",
          "js-sha256"
        ]
      }
    }
  }
}