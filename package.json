{
  "name": "cheer-app",
  "main": "expo-router/entry",
  "version": "1.2.4",
  "scripts": {
    "expo:doctor": "npx expo-doctor",
    "start": "npx tsc && expo start --tunnel",
    "start:web": "EXPO_PUBLIC_GIT_HASH=$(git rev-parse --short HEAD) npx expo export --platform web && npx serve dist",
    "deploy:web": "EXPO_PUBLIC_GIT_HASH=$(git rev-parse --short HEAD) npx expo export --platform web && npx firebase deploy --only hosting",
    "build:android": "npx eas build --platform android --profile production",
    "build:ios": "npx eas build --platform ios --profile production",
    "submit:android": "npx eas submit -p android --latest --profile production",
    "submit:ios": "npx eas submit --platform ios --latest --profile production",
    "reset-project": "node ./scripts/reset-project.js",
    "devbuild": "npx eas build -e development -p android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint"
  },
  "dependencies": {
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-navigation/native": "^7.1.6",
    "expo": "^55.0.4",
    "expo-build-properties": "~55.0.9",
    "expo-constants": "~55.0.7",
    "expo-dev-client": "~55.0.10",
    "expo-font": "~55.0.4",
    "expo-image-picker": "~55.0.10",
    "expo-linking": "~55.0.7",
    "expo-localization": "~55.0.8",
    "expo-router": "~55.0.3",
    "expo-splash-screen": "~55.0.10",
    "expo-status-bar": "~55.0.4",
    "expo-system-ui": "~55.0.9",
    "expo-updates": "~55.0.12",
    "expo-web-browser": "~55.0.9",
    "firebase": "^11.2.0",
    "i18n-js": "^4.4.3",
    "js-sha256": "^0.11.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-native": "0.83.2",
    "react-native-calendars": "^1.1306.0",
    "react-native-gesture-handler": "~2.30.0",
    "react-native-reanimated": "4.2.1",
    "react-native-safe-area-context": "~5.6.2",
    "react-native-screens": "~4.23.0",
    "react-native-toast-message": "^2.2.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "0.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/firebase": "^2.4.32",
    "@types/jest": "^29.5.12",
    "@types/react": "~19.2.10",
    "@types/react-test-renderer": "^18.3.0",
    "jest": "^29.7.0",
    "jest-expo": "~55.0.9",
    "react-test-renderer": "19.2.0",
    "typescript": "~5.9.2",
    "webpack": "^5.105.3",
    "webpack-cli": "^6.0.1"
  },
  "private": true,
  "overrides": {
    "@firebase/auth": {
      "@react-native-async-storage/async-storage": "2.2.0"
    }
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "firebase",
          "js-sha256"
        ]
      }
    }
  }
}